{
  "name": "UptimeEmitter",
  "version": "0.1.0",
  "description": "ping-monitor desktop app",
  "author": "Qawemlilo Mlilo <qawemlilo@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode=production --dest=./dist",
    "lint": "vue-cli-service lint",
    "dev": "NODE_ENV=DEV vue-cli-service serve & sleep 5 && NODE_ENV=DEV electron .",
    "start": "NODE_ENV=production electron ."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "electron-log": "^3.0.1",
    "electron-settings": "^3.2.0",
    "express": "^4.16.4",
    "material-design-icons": "^3.0.1",
    "nedb": "^1.8.0",
    "ping-monitor": "^0.4.1",
    "typeface-roboto": "0.0.54",
    "vue": "^2.6.4",
    "vuetify": "^1.5.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-stage-2": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.3",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.4",
    "vuetify-loader": "^1.2.0"
  },
  "babel": {
    "presets": [
      "@vue/app",
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "require": "readable",
      "import": "readable",
      "process": "readable"
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "MIT",
  "main": "main.js",
  "productName": "UptimeEmitter",
  "repository": "https://github.com/qawemlilo/uptime-emitter"
}
